
<% orders.forEach(order => { %>
<table class="demo-content--full demo-content--separated mdl-data-table mdl-shadow--2dp mdl-color--white">
  <thead>
    <tr>
      <th class="mdl-data-table__cell--non-numeric">Name</th>
      <th class="mdl-data-table__cell--non-numeric">Params</th>
      <th>Quantity</th>
      <th>Unit price</th>
      <th>Total price</th>
    </tr>
  </thead>
  <tbody>
    <% order.items.forEach(item => { %>
      <tr>
        <td class="mdl-data-table__cell--non-numeric"> <%= item.name %></td>
        <td class="mdl-data-table__cell--non-numeric"><%= item.params %></td>
        <td><%= item.quantity %></td>
        <td><%= item.price %></td>
        <td><%= item.price * item.quantity %></td>
      </tr>
    <% }) %>
  </tbody>
</table>

<% }) %>